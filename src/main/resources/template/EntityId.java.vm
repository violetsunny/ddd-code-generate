/**
 * ${comments}ID
 *
 * @author ${author}
 * @email ${email}
 * @date ${datetime}
 */
@Data
public class ${className}Id implements ValueObject<${className}Id> {

	private String id;

	public ${className}Id(final String id) {
		if(StringUtils.isEmpty(id)) {
			throw new IllegalArgumentException("${comments}id不能为空");
		}
		this.id = id;
	}

	public String getId() {
		return id;
	}

	@Override
